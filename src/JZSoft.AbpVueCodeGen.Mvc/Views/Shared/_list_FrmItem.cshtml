@model ParamDef

@if (Model.Name == "id")
{
    @*<el-form-item label="ID" v-if="(editModel.id)?true:false" prop="@Model.Name">
        <el-input type="hidden" v-model='editModel.id' /> 
    </el-form-item>*@
}
else
{   
    Model.Name = Model.Name.ToUpperStart();
    var lName = Model.Name.ToLowerStart();
    <el-form-item :label="L('@Html.Raw(Model.Name)')" prop="@Html.Raw(lName)">
        @switch (Model.Type)
        {
            case "string":
                if (Model.Format == "date-time")
                {
                    <el-date-picker v-model="editModel.@Html.Raw(lName)"
                                    type="date"
                                    placeholder="选择日期">
                    </el-date-picker>
                }
                else
                {
                    <el-input v-model='editModel.@Html.Raw(lName)' />
                }
                break;
            case "boolean":
                <el-checkbox v-model="editModel.@Html.Raw(lName)"></el-checkbox> 
                break;
            case "number":
            case "integer":
                <el-input type="number" v-model='editModel.@Html.Raw(lName)' />
                break;
            default:

                <!--复杂属性，请自己实现    @Model.Type-->
                <el-input type="text" v-model='editModel.@Html.Raw(lName)' />
                break;
        }
    </el-form-item>
}
